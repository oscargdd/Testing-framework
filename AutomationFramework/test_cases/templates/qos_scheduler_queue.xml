  <edit-config>
    <target>
      <{{target}}/>
    </target>
    <config>
      <qos xmlns="http://openconfig.net/yang/qos">
        <queues>
          <queue>
            <name>{{queue_name}}</name>
            <config>
              <name>{{queue_name}}</name>
            </config>
          </queue>
        </queues>
        <scheduler-policies>
          <scheduler-policy>
            <name>{{name}}</name>
            <config>
              <name>{{name}}</name>
            </config>
            <schedulers>
              <scheduler>
                <sequence>{{sequence}}</sequence>
                <config>
                  <sequence>{{sequence}}</sequence>
                </config>
                <inputs>
                  <input>
                    <id>{{id}}</id>
                    <config>
                      <id>{{id}}</id>
                      <queue>{{queue}}</queue>
                    </config>
                  </input>
                </inputs>
              </scheduler>
            </schedulers>
          </scheduler-policy>
        </scheduler-policies>
      </qos>
    </config>
  </edit-config>
